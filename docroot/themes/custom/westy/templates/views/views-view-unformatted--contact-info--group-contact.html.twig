<div{{ attributes }}>
  {{ title_prefix }}
  {{ title_suffix }}
  {% block content %}
  <div class="card contact-item">
    <div class="card-header">
      <h2>{{ view.getTitle() }}</h2>
    </div>
    <div class="card-body">
      {% for row in rows %}
        {{ row.content }}
      {% endfor %}
    </div>
  </div>
  {% endblock %}
</div>
